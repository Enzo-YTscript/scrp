-- Anti Afk
local vu = game:GetService("VirtualUser")
game:GetService("Players").LocalPlayer.Idled:connect(function()
    vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    wait(1)
    vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
end)
	
--- MAIN TAB	
    local TabAuto = Window:CreateTab("HOME", nil) -- Title, Image
	local SectionAuto = TabAuto:CreateSection("AutoClicker",false)

local AutoClicker = TabAuto:CreateToggle({
    Name = "Auto Clicker",
    SectionParent = SectionAuto,
    CurrentValue = false,
    Callback = function(v)
        if v then
            -- Aktifkan auto clicker
            AutoClicker = true
            while AutoClicker do
                game:GetService("ReplicatedStorage").Events.Click:FireServer()
                wait(000.1) -- Ubah nilai ini untuk mengatur kecepatan klik
            end
        else
            -- Matikan auto clicker
            AutoClicker = false
        end
    end
})

local AutoCollectDailyReward = TabAuto:CreateToggle({
    Name = "Auto Collect Daily Reward",
    SectionParent = SectionAuto,
    CurrentValue = false,
    Callback = function(v)
        if v then
            -- Aktifkan auto collect
            AutoCollect = true
            while AutoCollect do
                -- Cek apakah hadiah harian tersedia untuk diklaim
                local canClaim = game:GetService("ReplicatedStorage").Events.CanCollectDailyReward:InvokeServer()
                if canClaim then
                    game:GetService("ReplicatedStorage").Events.CollectDailyReward:FireServer()
                end
                wait(1) -- Ubah nilai ini untuk mengatur interval cek
            end
        else
            -- Matikan auto collect
            AutoCollect = false
        end
    end
})


-- TAB EGG
-- Fungsi untuk mendapatkan list telur dari folder EggHolders
local function GetEggList()
    local eggList = {}
    local eggHoldersFolder = game.Workspace:WaitForChild("Scripted-EggHolders") -- Pastikan nama folder sesuai
    for _, eggHolder in ipairs(eggHoldersFolder:GetChildren()) do
        if eggHolder:IsA("Model") then
            table.insert(eggList, eggHolder.Name)
        end
    end
    return eggList
end


local EggTab = Window:CreateTab("ðŸ¥š EGG", nil) -- Title, Image
local EggSection = EggTab:CreateSection("EGG")

-- Membuat dropdown dengan list telur dari folder EggHolders
local EggDropdown = EggTab:CreateDropdown({
    Name = "Select Area Earth",
	SectionParent = EggSection,
    Options = GetEggList(),
    CurrentOption = {""},
    Callback = function(Option)
        -- Fungsi untuk refresh dropdown dengan list yang baru
        local function RefreshDropdown()
            EggDropdown:Refresh(GetEggList(), {""})
        end
        
        -- Membuat tombol toggle untuk auto hatch telur dari dropdown
        local AutoHatchToggle = EggTab:CreateToggle({
            Name = "Auto Hatch " .. Option .. " (Triple)",
            SectionParent = EggSection,
            CurrentValue = false,
            Callback = function(v)
                if v then
                    -- Aktifkan auto hatch
                    AutoHatch = true
                    while AutoHatch do
                        -- Cek apakah telur tersedia di folder EggHolders
                        local eggHoldersFolder = game.Workspace:WaitForChild("Scripted-EggHolders")
                        local egg = eggHoldersFolder:FindFirstChild(Option)
                        if egg then
                            -- Lakukan tindakan triple hatch
                            for _ = 1, 3 do
                                egg:FindFirstChild("HatchButton"):Invoke()
                                wait(0.5) -- Tunggu sebentar antara setiap hatch
                            end
                        end
                        wait(1) -- Ubah nilai ini untuk mengatur interval hatch
                    end
                else
                    -- Matikan auto hatch
                    AutoHatch = false
                end
            end
        })

        -- Membuat tombol refresh dropdown
        local RefreshButton = EggTab:CreateButton({
            Name = "Refresh Dropdown",
            SectionParent = EggSection,
            Text = "Refresh",
            Callback = RefreshDropdown
        })
    end
})

--TAB TELEPORT
	local Tab3 = Window:CreateTab("Teleport") -- Title, Image	
	local Telep = Tab3:CreateSection("Teleport",false)
	local Label = Tab3:CreateLabel("Need Teleport Gempass",Telep)	
	
-- Membuat dropdown Realm
local RealmDropdown = Tab3:CreateDropdown({
	Name = "Select Realm",
	SectionParent = Telep,
    Options = {"Earth", "Cyberverse"},
	Flag = "Dropdown5",
	CurrentOption = {""},
    Callback = function(Option)
        getgenv().selectedRealm = Option[1]
    end,
})

-- Membuat dropdown Area untuk Earth
local EarthAreaDropdown = Tab3:CreateDropdown({
	Name = "Select Area Earth",
    Options = {"Town", "Frosty Kingdom", "Desert Adventure", "Jungle Expedition", "Pirate Cove", "Cerberus Realm",
    "Dinosaur Kingdom", "Farm Life", "Mountain Expedition", "Fantasy Forest", "Aqua Land", "Hydra Room", "The Portal"}, -- Ganti dengan daftar Area untuk Earth
    CurrentOption = {""},
	SectionParent = Telep,	
	Flag = "Dropdown6",
Callback = function(Option)
        if getgenv().selectedRealm == "Earth" then
            getgenv().selectedAreaEarth = Option[1]
        end
    end,
})

-- Membuat dropdown Area untuk CyberTown
local CyberTownAreaDropdown = Tab3:CreateDropdown({
	Name = "Select Area CyberTown",
    Options = {"Cyber Town", "Robo Road", "Cyber Adventure", "Robo Expedition", "Cyborg Cerberus Room", "Cyber Kingdom",
    "Robo Life", "Cyber Expedition", "Robo Forest", "Robo Land", "Robo Hydra Room", "The Second Portal",}, -- Ganti dengan daftar Area untuk CyberTown
    CurrentOption = {""},
	SectionParent = Telep,	
	Flag = "Dropdown7",
    Callback = function(Option)
        if getgenv().selectedRealm == "Cyberverse" then
            getgenv().selectedAreaCyberTown = Option[1]
        end
    end,
})

-- Membuat tombol Teleport
local TeleportButton = Tab3:CreateButton({
    Name = "Teleport",
	SectionParent = Telep,	
    Callback = function()
if getgenv().selectedRealm and ((getgenv().selectedRealm == "Earth" and getgenv().selectedAreaEarth) or (getgenv().selectedRealm == "Cyberverse" and getgenv().selectedAreaCyberTown)) then
            local selectedArea = getgenv().selectedRealm == "Earth" and getgenv().selectedAreaEarth or getgenv().selectedAreaCyberTown
            local args = {
                [1] = getgenv().selectedRealm,
                [2] = selectedArea
            }            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Teleport"):FireServer(unpack(args))
        else
            warn("Pilih Realm dan Area terlebih dahulu.")
        end
    end,
})

